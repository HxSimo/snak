{
  "name": "starknet-agent-kit",
  "version": "0.0.10",
  "workspaces": [
    "packages/*"
  ],
  "description": "A toolkit for creating AI agents that can interact with the Starknet blockchain",
  "author": "Kasar Labs",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasarlabs/starknet-agent-kit.git"
  },
  "keywords": [
    "starknet",
    "blockchain",
    "AI",
    "agent",
    "nestjs"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "clean:all": "pnpm clean && pnpm -r clean:all",
    "build:backend": "rm -rf dist && nest build",
    "clean:js": "rm -rf \"agent/**/*.js\"",
    "local" : "pnpm --filter \"agent\" local",
    "build:test": "pnpm clean:js && tsc -p tsconfig.build.json agent/local.ts --outDir ./dist",
    "build:frontend": "pnpm --filter \"client\" build",
    "build:lib": "tsc -p tsconfig.build.json",
    "build": "lerna run build",
    "dev:backend": "nest start --watch",
    "dev:frontend": "pnpm --filter \"client\" dev",
    "dev": "concurrently \"pnpm dev:backend\" \"pnpm dev:frontend\"",
    "start:backend": "node dist/main",
    "start:frontend": "pnpm --filter \"client\" start",
    "start": "concurrently \"pnpm start:backend\" \"pnpm start:frontend\"",
    "lint": "eslint \"{agent,apps,libs,test}/**/*.ts\" --fix",
    "lint:frontend": "pnpm --filter \"client\" build lint",
    "test": "jest --passWithNoTests",
    "test:frontend": "pnpm --filter \"client\" build test",
    "setup": "./scripts/setup.sh",
    "dev:script": "./scripts/dev.sh",
    "start:script": "./scripts/start.sh",
    "prepublishOnly": "pnpm clean && pnpm install && pnpm run build:lib",
    "test:lib": "ts-node -r tsconfig-paths/register scripts/test-lib.ts",
    "test:lib:local": "./scripts/test-release.sh"
  },
  "dependencies": {
    "@fastify/multipart": "^9.0.3",
    "add": "^2.0.6",
    "boxen": "^8.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "express": "^4.21.2",
    "fastify": "^5.2.1",
    "gradient-string": "^3.0.0",
    "helmet": "^8.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "starknet": "^6.11.0",
    "zod": "^3.24.1"

  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^11.0.6",
    "@types/express": "^5.0.0",
    "@types/gradient-string": "^1.1.6",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.17.12",
    "@types/supertest": "^6.0.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^15.14.0",
    "jest": "^29.5.0",
    "lerna": "^8.1.9",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.19.1"
    
  }
}
